{
  "/google-api": {
    "target": "https://www.googleapis.com",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/google-api": ""
    },
    "logLevel": "debug",
    "headers": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Credentials": "true"
    }
  },
  "/gapi": {
    "target": "https://apis.google.com",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": {
      "^/gapi": ""
    },
    "logLevel": "debug",
    "headers": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Credentials": "true"
    }
  },
  "/api": {
    "target": "http://localhost:8081",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": ""
    },
    "logLevel": "debug"
  },
  "/backend": {
    "target": "http://localhost:8083",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/backend": ""
    },
    "logLevel": "debug",
    "headers": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Credentials": "true"
    }
  }
}
